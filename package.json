{
  "name": "@hisorange/inductor",
  "version": "2.0.0",
  "description": "Bidirectonal database manager.",
  "keywords": [
    "data-model",
    "database",
    "inspector",
    "orm",
    "postgres",
    "postgresql",
    "schema",
    "reflection"
  ],
  "main": "build/index.js",
  "repository": "https://github.com/hisorange/inductor",
  "author": "Zsolt Varga <hello@hisorange.me>",
  "engine": {
    "node": ">=16.0.0"
  },
  "license": "GPL-3.0",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "tsc --project ./tsconfig.build.json",
    "db:logs": "docker-compose logs --no-log-prefix --tail 100 -f",
    "db:reset": "yarn db:stop && yarn db:start",
    "db:start": "docker-compose up -d --renew-anon-volumes",
    "db:stop": "docker-compose down",
    "test:types": "tsc --noEmit",
    "test": "jest -i",
    "gen:test": "hygen pg-test new"
  },
  "devDependencies": {
    "@types/jest": "^28.0.0",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/node": "^16.0.0",
    "jest": "^28.0.0",
    "objection": "^3.0.1",
    "pg": "^8.7.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.0",
    "typescript": "^4.7.0"
  },
  "dependencies": {
    "just-diff": "^5.0.1",
    "knex": "^2.2.0",
    "lodash.clonedeep": "^4.5.0",
    "nanoid": "^4.0.1",
    "objection": "^3.0.1",
    "pg": "^8.7.3",
    "pino": "^7.11.0"
  }
}
