{
  "name": "@hisorange/inductor",
  "version": "1.0.0",
  "description": "Bidirectonal PostgreSQL specialized database manager.",
  "keywords": [
    "orm",
    "postgresql",
    "postgres",
    "database",
    "schema",
    "data-model",
    "inspector"
  ],
  "main": "build/index.js",
  "repository": "https://github.com/hisorange/inductor",
  "author": "Zsolt Varga <hello@hisorange.me>",
  "license": "GPL-3.0",
  "scripts": {
    "build": "tsc --build ./tsconfig.json",
    "test": "jest -i",
    "db:start": "docker-compose up -d --renew-anon-volumes",
    "db:stop": "docker-compose down",
    "db:reset": "yarn db:stop && yarn db:start",
    "db:logs": "docker-compose logs --no-log-prefix --tail 100 -f"
  },
  "devDependencies": {
    "@types/jest": "^27.0.0",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/node": "^16.0.0",
    "@types/uuid": "^8.3.4",
    "jest": "^27.0.0",
    "lodash.clonedeep": "^4.5.0",
    "ts-jest": "^27.0.0",
    "typescript": "^4.6.0"
  },
  "dependencies": {
    "just-diff": "^5.0.1",
    "knex": "^2.0.0",
    "knex-schema-inspector": "^1.7.5",
    "objection": "^3.0.1",
    "pg": "^8.7.3",
    "uuid": "^8.3.2"
  }
}
