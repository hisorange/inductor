{
  "name": "@hisorange/inductor",
  "version": "1.5.0",
  "description": "Bidirectonal PostgreSQL specialized database manager.",
  "keywords": [
    "data-model",
    "database",
    "inspector",
    "orm",
    "postgres",
    "postgresql",
    "schema"
  ],
  "main": "build/index.js",
  "repository": "https://github.com/hisorange/inductor",
  "author": "Zsolt Varga <hello@hisorange.me>",
  "engine": {
    "node": ">=16.0.0"
  },
  "license": "GPL-3.0",
  "scripts": {
    "build": "tsc --build ./tsconfig.build.json",
    "db:logs": "docker-compose logs --no-log-prefix --tail 100 -f",
    "db:reset": "yarn db:stop && yarn db:start",
    "db:start": "docker-compose up -d --renew-anon-volumes",
    "db:stop": "docker-compose down",
    "test:types": "tsc --noEmit",
    "test": "jest -i",
    "gen:pg-test": "hygen pg-test new"
  },
  "devDependencies": {
    "@types/jest": "^28.0.0",
    "@types/lodash.clonedeep": "^4.5.7",
    "@types/node": "^16.0.0",
    "@types/uuid": "^8.3.4",
    "hygen": "^6.2.8",
    "jest": "^28.0.0",
    "lodash.clonedeep": "^4.5.0",
    "ts-jest": "^28.0.0",
    "typescript": "^4.7.0"
  },
  "dependencies": {
    "eventemitter2": "^6.4.7",
    "just-diff": "^5.0.1",
    "pino": "^7.11.0",
    "uuid": "^8.3.2"
  },
  "optionalDependencies": {
    "knex": "^2.2.0",
    "knex-schema-inspector": "^2.0.3",
    "objection": "^3.0.1",
    "pg": "^8.7.3"
  }
}
